@model List<AppUser>
<table class="table table-sm table-bordered table-bordered">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>RoleAssign</th>
        
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td><a asp-action="roleassign" asp-controller="home" asp-area="management" asp-route-id="@user.Id" class="btn btn-danger">RoleAssign</a></td>
        </tr>
    }
</table>